        <div class="col-3">
          {{ include('title.html.twig', {title: '视频', noMore: 1, haveRedBar: 1}) }}
            <div class="mt-1">
              <img class="img-fluid" src="img/274x186.png">
            </div>
              {{ include('list.html.twig', {count: 8}) }}
        </div>
      </section>

      <section class="row mx-0 my-1" id="row-7">
        <div class="overflow-hidden">
          {{ include('title.html.twig', {title: '摄影展览'}) }}
          {{ include('gallery.html.twig') }}
        </div>
      </section>

      <section class="row mx-0 my-1" id="row-8">
        <div class="col px-0">
          {{ include('title.html.twig', {title: '相关链接', noMore: 1}) }}
          {{ include('links.html.twig') }}

        </div>
      </section>

      <section class="border-top-red" style="">

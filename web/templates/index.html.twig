              <div class="row">
                <a class="col btn btn-primary mt-1 mx-1">管理系统</a>
                <a class="col btn btn-primary mt-1 mx-1">管理系统</a>
              </div>
          </div>
      </section>

      <section class="row mb-1 px-0">
          <div class="col">
            <div class="row">
              <div class="col">
                {{ include('title.html.twig', {title: '审判管理'}) }}
                {{ include('list.html.twig', {count: 5}) }}
              </div>
              <div class="col">
                {{ include('title.html.twig', {title: '执行动态'}) }}
                {{ include('list.html.twig', {count: 5}) }}
              </div>
            </div>
            <div class="row">
              <div class="col">
                {{ include('title.html.twig', {title: '案件快报'}) }}
                {{ include('list.html.twig', {count: 5}) }}
              </div>
              <div class="col">
                {{ include('title.html.twig', {title: '诉讼服务'}) }}
                {{ include('list.html.twig', {count: 5}) }}
              </div>
            </div>
          </div>
          <div class="col-3">
            <div>
              {{ include('title.html.twig', {title: '法院概况', noMore: 1, haveRedBar: 1}) }}
              {{ include('figure.html.twig', {cap: '法院概述', src: 'img/274x160.png'}) }}
              {{ include('figure.html.twig', {cap: '党委书记、院长', src: 'img/274x160.png'}) }}
            </div>
          </div>
      </section>


      <section class="row mx-0 my-1" id="pics">
        <div class="overflow-hidden">
          {{ include('title.html.twig', {title: '图说法院'}) }}
          {{ include('gallery.html.twig', {haveCaption: 1}) }}
        </div>
      </section>

      <section class="row mb-1" id="aboutus">
        <div class="col">
          <div class="row mb-1">
            <div class="col">
              {{ include('title.html.twig', {title: '党建专栏'}) }}
              {{ include('list.html.twig', {count: 6}) }}

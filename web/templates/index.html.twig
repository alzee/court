  </head>
  <body class="container py-4">
    <header class="mb-1">
        <img class="img-fluid" src="img/1200x180.png">
    </header>
      {{ include('nav.html.twig')}}
    <main class="mt-1">
      <section class="mb-1" id="banner">
        <img class="img-fluid" src="img/1200x90.png">
      </section>

      <section class="row mb-1" id="slider">
        <div class="col">
          <img class="img-fluid rounded" src="img/600x400.png">
        </div>
        <div class="col">
          {{ include('title.html.twig', {title: '新闻中心'}) }}

          {{ include('list.html.twig', {ulClass: 'font-weight-bold', count: 4, isNew: 1}) }}

          {{ include('list.html.twig', {ulClass: 'border-top border-primary', count: 6}) }}
        </div>
      </section>

      <section class="row mb-1 px-0">
          <div class="col">
            <div class="row">
              <div class="col">
                {{ include('title.html.twig', {title: '通知'}) }}
                {{ include('list.html.twig', {count: 4}) }}
              </div>
              <div class="col">
                {{ include('title.html.twig', {title: '公告'}) }}
                {{ include('list.html.twig', {count: 4}) }}
              </div>
            </div>
            <div class="row">
              <div class="col">
                {{ include('title.html.twig', {title: '法院文件'}) }}
                {{ include('list.html.twig', {count: 4}) }}
              </div>
              <div class="col">
                {{ include('title.html.twig', {title: '工作动态'}) }}
                {{ include('list.html.twig', {count: 4}) }}
              </div>
            </div>
          </div>
          <div class="col-3">
            {{ include('title.html.twig', {title: '办公业务平台', noMore: 1, haveRedBar: 1}) }}
              <div class="">
                <a class="btn btn-primary mt-1 d-block">法院信息管理系统</a>
                <a class="btn btn-primary mt-1 d-block">法院信息管理系统</a>
                <a class="btn btn-primary mt-1 d-block">法院信息管理系统</a>
                <a class="btn btn-primary mt-1 d-block">法院信息管理系统</a>
